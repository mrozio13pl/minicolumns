{
  "name": "minicolumns",
  "version": "1.0.0",
  "description": "Create simple text columns.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": "mrozio13pl/minicolumns",
  "author": "mrozio13pl",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build && npm t",
    "build": "tsup src/index.ts --clean --dts --minify --format esm,cjs",
    "test": "uvu test .js$"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "text",
    "table",
    "ansi",
    "column",
    "row"
  ],
  "dependencies": {
    "pads": "^1.1.0",
    "strip-ansi": "6.0.1"
  },
  "devDependencies": {
    "@mrozio/eslint-config": "^1.0.1",
    "eslint": "^8.51.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6"
  },
  "packageManager": "^pnpm@8.9.0"
}